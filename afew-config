# vim:set ft=dosini:

[SpamFilter]
# Tags mails flagged as SPAM

[KillThreadsFilter]
# Kill replies to killed mails

[ListMailsFilter]
# Add list/<id> tags for mailing lists

[ArchiveSentMailsFilter]
# Mark e-mails from me as 'sent'
sent_tag = sent

[Filter.9]
message = Finish tagging by removing the new tag
tags = -new
